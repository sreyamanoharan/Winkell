	<section>
		<div id="carouselExampleIndicators" class="carousel slide" data-ride="carousel">
			<ol class="carousel-indicators">
			  <li data-target="#carouselExampleIndicators" data-slide-to="0" class="active"></li>
			  <li data-target="#carouselExampleIndicators" data-slide-to="1"></li>
			  <li data-target="#carouselExampleIndicators" data-slide-to="2"></li>
			</ol>
			<div class="carousel-inner">
			  <div class="carousel-item active">
				<img class="d-block w-100" src="/banner/<%=banner[0]?.bannerImg  %>" alt="First slide">
				<div class="carousel-caption d-none d-md-block">
					<h5><%=banner[0].main %></h5>
					<p><%=banner[0].sub %></p>
				  </div>
			  </div>
			  <% banner.forEach(data=>{%>
				<div class="carousel-item">
					<img class="d-block w-100" src="/banner/<%=data.bannerImg  %>" alt="Second slide">
					<div class="carousel-caption d-none d-md-block">
						<h5><%=data.main %></h5>
						<p><%=data.sub  %></p>
					  </div>
				  </div>
			  <%});%>
			  
			  
			</div>
			<a class="carousel-control-prev" href="#carouselExampleIndicators" role="button" data-slide="prev">
			  <span class="carousel-control-prev-icon" aria-hidden="true"></span>
			  <span class="sr-only">Previous</span>
			</a>
			<a class="carousel-control-next" href="#carouselExampleIndicators" role="button" data-slide="next">
			  <span class="carousel-control-next-icon" aria-hidden="true"></span>
			  <span class="sr-only">Next</span>
			</a>
		  </div>
	</section>

    <section class="ftco-section bg-light">
    	<div class="container">
				<div class="row justify-content-center mb-3 pb-3">
          <div class="col-md-12 heading-section text-center ftco-animate">
            <h2 class="mb-4">Latest Arrival</h2>
            <p>Far far away, behind the word mountains, far from the countries Vokalia and Consonantia</p>
          </div>
        </div>   		
    	</div>
    	<div class="container">
    		<div class="row">
    			
				<% products.forEach( data => { %>
					<div class="col-sm-6 col-md-6 col-lg-4 ftco-animate">
					  
						 
					
						<div class="product">
							<a href=""  class="img-prod"><img class="img-fluid" src="/imagess/<%=data.image %>" alt="Colorlib Template">
								<div class="overlay"></div>
							</a>
							<div class="text py-3 px-3">
								<h3><a href="#"></a> <%=data.name  %></h3>
								<div class="d-flex">
									<div class="pricing">
										
										<% if(data.offerStatus){ %>
											<s><span class="price text-dark">₹<%=data.originalPrice %></span></s> <span class="price text-dark">₹<%= data.price %></span>
										<% }else{ %>
											<s><span class="price text-dark">₹<%=data.originalPrice %></span></s><p class="price"><span>₹<%=data.price %></span></p>
										<% } %>
									</div>
									<div class="rating">
										<p class="text-right">
											<a href="#"><span class="ion-ios-star-outline"></span></a>
											<a href="#"><span class="ion-ios-star-outline"></span></a>
											<a href="#"><span class="ion-ios-star-outline"></span></a>
											<a href="#"><span class="ion-ios-star-outline"></span></a>
											<a href="#"><span class="ion-ios-star-outline"></span></a>
										</p>
									</div>
								</div>
								<p class="bottom-area d-flex px-3">
									
									<!-- <a  class="add-to-cart text-center py-2 mr-1" onclick="addToCart('<%= data._id %>')" ><span><i class="ion-ios-add ml-1"></i></span></a> -->
									
								</p>
							</div>
						</div>   
					</div>
					<% }) %>
    		
    		</div>
    	</div>
    </section>

    

   

  

